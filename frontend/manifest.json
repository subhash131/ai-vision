{
  "manifest_version": 3,
  "name": "A Eye",
  "version": "1.0.0",
  "description": "AI eye for browsing",
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.tsx"]
    }
  ],
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  }
}
